    "    \"\"\"Fungsi rekursi untuk menentukan faktorial bilangan\"\"\"\n",
    "    if x == 1:\n",
    "        return 1\n",
    "    else:\n",
    "        return (x * faktorial(x-1))\n",
    "bil = 4\n",
    "print(\"Faktorial dari\", bil, \"adalah\", faktorial(bil))"
